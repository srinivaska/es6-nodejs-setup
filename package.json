  "author": "srinivas kandukuri",
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.1",
    "express": "^4.16.3",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "codecov": "^3.1.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1"
  }
}
